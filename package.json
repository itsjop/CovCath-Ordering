{
  "name": "covCath-Ordering",
  "version": "1.0.0",
  "description": "My sensational Nuxt.js project",
  "author": "Jordan Crandall",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon src/server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node src/server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "build:firebase": "yarn clean && yarn build && yarn copy && cd \"functions\" && yarn",
    "clean": "yarn clean:public && yarn clean:functions && yarn clean:static",
    "clean:functions": "rimraf \"functions/node_modules\" && rimraf \"functions/.nuxt\"",
    "clean:public": "rimraf \"public/**/*.*!(md)\" && rimraf \"public/_nuxt\"",
    "clean:static": "rimraf \"src/static/sw.js\"",
    "copy": "yarn copy:nuxt && yarn copy:static",
    "copy:nuxt": "xcopy \"functions\\.nuxt\\dist\\*\" \"public\\_nuxt\\\" /E /Y",
    "copy:static": "xcopy \"src\\static\\*\" \"public\\\" /E /Y",
    "start:firebase": "firebase serve --only functions,hosting",
    "deploy": "firebase deploy --only functions,hosting"
  },
  "dependencies": {
    "@nuxtjs/auth": "^4.9.1",
    "@nuxtjs/axios": "^5.10.3",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "@nuxtjs/toast": "^3.3.0",
    "browserify-fs": "^1.0.0",
    "cross-env": "^5.2.0",
    "debounce": "^1.2.0",
    "express": "^4.16.4",
    "firebase": "^7.14.3",
    "firebase-admin": "^8.12.1",
    "firebaseui": "^4.5.0",
    "human-readable-ids": "^1.0.4",
    "net": "^1.0.2",
    "node-fetch": "^2.6.0",
    "nuxt": "^2.0.0",
    "nuxt-stripe-module": "^2.0.0",
    "shortid": "^2.2.15",
    "stripe": "^8.52.0",
    "vue-stripe-checkout": "^3.3.1",
    "vue-stripe-elements-plus": "^0.3.2",
    "vue-tables-2": "^2.0.23",
    "vue-tailwind": "^0.4.6",
    "vue-tailwind-modal": "0.0.7",
    "vue-the-mask": "^0.11.1",
    "vuefire": "^2.2.2",
    "vuetify": "^2.2.27",
    "vuetify-loader": "^1.0.8"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^0.0.1",
    "@nuxtjs/tailwindcss": "^1.0.0",
    "@nuxtjs/vuetify": "^1.11.2",
    "babel-eslint": "^8.2.1",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": ">=2.14.0",
    "eslint-plugin-jest": ">=21.24.1",
    "eslint-plugin-node": ">=7.0.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "nodemon": "^1.18.9",
    "prettier": "1.14.3",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "eslint-plugin-vue": "^5.0.0"
  }
}
